<DungeonConfiguration>
  <roomShapes>
    <roomShapes>
      <name>small_rect</name>
      <template>
        <name>rectangle</name>
        <params>
          <size-x>5-10</size-x>
          <size-y>4-8</size-y>
        </params>
      </template>
      <shape/>
    </roomShapes>
    <roomShapes>
      <name>small_sq</name>
      <template>
        <name>square</name>
        <params>
          <size>5-10</size>
        </params>
      </template>
      <shape/>
    </roomShapes>
    <roomShapes>
      <name>heart</name>
      <template/>
      <shape>..x...x..</shape>
      <shape>.xxx.xxx.</shape>
      <shape>xxxxxxxxx</shape>
      <shape>.xxxxxxx.</shape>
      <shape>..xxxxx..</shape>
      <shape>...xxx...</shape>
      <shape>....x....</shape>
    </roomShapes>
    <roomShapes>
      <name>diamond</name>
      <template/>
      <shape>.....x.....</shape>
      <shape>....xxx....</shape>
      <shape>...xxxxx...</shape>
      <shape>..xxxxxxx..</shape>
      <shape>.xxxxxxxxx.</shape>
      <shape>xxxxxxxxxxx</shape>
      <shape>.xxxxxxxxx.</shape>
      <shape>..xxxxxxx..</shape>
      <shape>...xxxxx...</shape>
      <shape>....xxx....</shape>
      <shape>.....x.....</shape>
    </roomShapes>
    <roomShapes>
      <name>big_square</name>
      <template>
        <name>square</name>
        <params>
          <size>20-25</size>
        </params>
      </template>
      <shape/>
    </roomShapes>
    <roomShapes>
      <name>mega room</name>
      <template>
        <name>rectangle</name>
        <params>
          <size-x>50-75</size-x>
          <size-y>50-75</size-y>
        </params>
      </template>
      <shape/>
    </roomShapes>
  </roomShapes>
  <rooms>
    <rooms>
      <name>basic</name>
      <shape>small_rect</shape>
      <amount>500-1000</amount>
      <rotate>false</rotate>
    </rooms>
    <rooms>
      <name>basic2</name>
      <shape>small_sq</shape>
      <amount>500-1000</amount>
      <rotate>false</rotate>
    </rooms>
    <rooms>
      <name>heart</name>
      <shape>heart</shape>
      <amount>200-400</amount>
      <rotate>true</rotate>
    </rooms>
    <rooms>
      <name>diamond</name>
      <shape>diamond</shape>
      <amount>100-200</amount>
      <rotate>true</rotate>
    </rooms>
    <rooms>
      <name>boss</name>
      <shape>big_square</shape>
      <amount>10-50</amount>
      <rotate>false</rotate>
    </rooms>
    <rooms>
      <name>mega</name>
      <shape>mega room</shape>
      <amount>1-10</amount>
      <rotate>false</rotate>
    </rooms>
  </rooms>
  <outputFileType>XML</outputFileType>
  <seed/>
</DungeonConfiguration>
