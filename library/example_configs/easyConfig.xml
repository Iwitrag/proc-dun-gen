<DungeonConfiguration>
  <roomShapes>
    <roomShapes>
      <name>small_rect</name>
      <template>
        <name>rectangle</name>
        <params>
          <size-x>5-10</size-x>
          <size-y>4-8</size-y>
        </params>
      </template>
      <shape/>
    </roomShapes>
    <roomShapes>
      <name>heart</name>
      <template/>
      <shape>..x...x..</shape>
      <shape>.xxx.xxx.</shape>
      <shape>xxxxxxxxx</shape>
      <shape>.xxxxxxx.</shape>
      <shape>..xxxxx..</shape>
      <shape>...xxx...</shape>
      <shape>....x....</shape>
    </roomShapes>
    <roomShapes>
      <name>big_square</name>
      <template>
        <name>square</name>
        <params>
          <size>20-25</size>
        </params>
      </template>
      <shape/>
    </roomShapes>
  </roomShapes>
  <rooms>
    <rooms>
      <name>basic</name>
      <shape>small_rect</shape>
      <amount>10-20</amount>
      <rotate>false</rotate>
    </rooms>
    <rooms>
      <name>heart</name>
      <shape>heart</shape>
      <amount>5-10</amount>
      <rotate>true</rotate>
    </rooms>
    <rooms>
      <name>boss</name>
      <shape>big_square</shape>
      <amount>0-3</amount>
      <rotate>false</rotate>
    </rooms>
  </rooms>
  <outputFileType>XML</outputFileType>
  <seed/>
</DungeonConfiguration>
